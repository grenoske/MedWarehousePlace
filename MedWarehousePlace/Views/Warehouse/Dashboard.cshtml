<div class="card text-center">
    <div class="card-header">
        <h5 class="card-title">Warehouse 1</h5>
        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
        <a asp-controller="Warehouse" asp-action="Index" class="btn btn-primary">Change Warehouse</a>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Medica at risk of expiration</h5>
                        <ul class="list-group">
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Expired
                                <span class="badge bg-primary bg-pill">14</span>
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Expiring This Week
                                <span class="badge bg-primary bg-pill">2</span>
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Expiring This Month
                                <span class="badge bg-primary bg-pill">1</span>
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                Expiring This Year
                                <span class="badge bg-primary bg-pill">1</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Medicines running low</h5>
                        <ul>
                            <li>Med A - Remaining quantity: 10</li>
                            <li>Med B - Remaining quantity: 5</li>
                        </ul>
                        <a href="#" class="btn btn-primary">View more</a>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Latest unload to receiving area</h5>
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">Stethoscope - quantity: 2</li>
                            <li class="list-group-item">Saline 0.9% 100ml - quantity: 5</li>
                        </ul>
                        <p class="mt-2">Total items on Receiving area: 10</p>
                        <a href="#" class="btn btn-primary">View All</a>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Moved to Packaging</h5>
                        <ul class="list-group">
                            <li class="list-group-item">MRI Machine - quantity: 1</li>
                            <li class="list-group-item">Ultrasound Scanner - quantity: 1</li>
                        </ul>
                        <p class="mt-2">Total items on Packing area: 7</p>
                        <a href="#" class="btn btn-primary">View All</a>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Warehouse Occupancy</h5>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">25 bins</div>
                        </div>
                        <p class="mt-2">Available space: 75 bins</p>
                        <a href="#" class="btn btn-primary">Manage space</a>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">High Turnover Items</h5>
                        <ul class="list-group">
                            <li class="list-group-item">Gloves - quantity: 500</li>
                            <li class="list-group-item">Syringes - quantity: 800</li>
                            <li class="list-group-item">Bandages - quantity: 600</li>
                        </ul>
                        <p class="mt-2">Total items: 1900</p>
                        <a href="#" class="btn btn-primary">View All</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
